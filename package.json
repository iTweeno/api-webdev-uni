    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.1",
